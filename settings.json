{
    "workbench.colorTheme": "Deepdark Material Theme | Full Black Version",
    "workbench.startupEditor": "none",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "editor.fontFamily": "'JetBrains Mono',Menlo, Monaco, 'Courier New', monospace",
    "breadcrumbs.enabled": false,
    "editor.minimap.enabled": true,
    "workbench.sideBar.location": "left",
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "arduino.path": "/Applications",
    "arduino.additionalUrls": "https://raw.githubusercontent.com/VSChina/azureiotdevkit_tools/master/package_azureboard_index.json",
    "explorer.confirmDragAndDrop": false,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "explorer.confirmDelete": false,
    "extensions.ignoreRecommendations": true,
    "editor.mouseWheelZoom": true,
    "workbench.statusBar.visible": true,
    "workbench.activityBar.visible": true,
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "files.exclude": {
        "**/.DS_Store": false,
        "**/.git": true,
        "**/node_modules": false,
        "node_modules": false
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.tsdk": "",
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "workbench.editor.showTabs": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "[yaml]": {},
    "terminal.external.osxExec": "iTerm2.app",
    "terminal.integrated.fontFamily": "'Hack','MesloLGS NF'",
    "workbench.panel.defaultLocation": "right",
    "terminal.integrated.cursorStyle": "underline",
    "vim.cursorStylePerMode.insert": "line",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.visual": "block",
    "vim.statusBarColors.surroundinputmode": "",
    "vim.surround": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<S-h>"
            ],
            "commands": [
                "workbench.action.previousEditor"
            ]
        },
        {
            "before": [
                "<S-l>"
            ],
            "commands": [
                "workbench.action.nextEditor"
            ]
        }
    ],
    "editor.lineNumbers": "relative",
    "workbench.colorCustomizations": {
        "editorLineNumber.foreground": "#b2b2b2"
    },
    "editor.tabCompletion": "on",
    "IoTWorkbench.workbench": "/Users/tkim/Documents/IoTWorkbenchProjects",
    "window.zoomLevel": 1,
    "redhat.telemetry.enabled": false,
    "terminal.explorerKind": "external",
    "debug.terminal.clearBeforeReusing": true
}