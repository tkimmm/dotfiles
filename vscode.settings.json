{
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "editor.fontFamily": "'Hack',Menlo, Monaco, 'Courier New', monospace",
    "breadcrumbs.enabled": false,
    "editor.minimap.enabled": false,
    "workbench.sideBar.location": "left",
    "window.zoomLevel": 1,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "explorer.confirmDragAndDrop": false,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.startupEditor": "newUntitledFile",
    "explorer.confirmDelete": false,
    "extensions.ignoreRecommendations": true,
    "editor.mouseWheelZoom": true,
    "workbench.statusBar.visible": true,
    "workbench.activityBar.visible": true,
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "files.exclude": {
        "**/.DS_Store": false,
        "**/.git": true,
        "**/node_modules": false,
        "node_modules": false
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "azureFunctions.showProjectWarning": false,
    "azureFunctions.showCoreToolsWarning": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "appService.showSavePrompt": false,
    "dart.openDevTools": "flutter",
    "typescript.tsdk": "",
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "workbench.editor.showTabs": true,
    "azure-iot-edge.EdgeHubConnectionString": "HostName=tkim.azure-devices.net;GatewayHostName=localhost;DeviceId=tkimkube;ModuleId=target;SharedAccessKey=7xQWny1owYMbIenB1lpkGZacQGzt1L0GxBF6sPTWQqU=",
    "azure-iot-edge.EdgeModuleCACertificateFile": "/var/lib/iotedgehubdev/certs/edge-device-ca/cert/edge-device-ca.cert.pem",
    "workbench.colorTheme": "Celestial",
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "githubPullRequests.remotes": [
        "origin",
        "upstream",
        "github"
